CREATE TABLE `students` (
	`STUDENT_ID` INT(11) UNSIGNED ZEROFILL NOT NULL AUTO_INCREMENT,
	`STUDENT_FIRST_NAME` VARCHAR(50) NOT NULL DEFAULT '\'FN\'',
	`STUDENT_LAST_NAME` VARCHAR(50) NOT NULL DEFAULT '\'LN\'',
	`PHONE` INT(10) NULL DEFAULT NULL,
	`CREATION` DATETIME NOT NULL,
	`DATE_OF_RECORD` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP,
	`DATE_OF_LAST_CHANGE` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
	`STATUS` ENUM('Y','N') NOT NULL,
	INDEX `Index 1` (`STUDENT_ID`)
)
ENGINE=InnoDB
;
